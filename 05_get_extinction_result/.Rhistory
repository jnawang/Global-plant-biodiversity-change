extinction_thomas[scenario-4, 5] <- 1.0 - mean(pmin(value_MAXE[, 2+scenario], 1.0), na.rm=T)^z
extinction_thomas[scenario-4, 6] <- 1.0 - mean(pmin(value_MAXE[, 2+scenario+16*4], 1.0), na.rm=T)^z
extinction_thomas[scenario-4, 7] <- 1.0 - mean(pmin(value_MAXE[, 2+scenario+16], 1.0), na.rm=T)^z
