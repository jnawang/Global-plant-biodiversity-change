for (scenario in scenarios) {
extinction_thomas[scenario-4, 2] <- 1.0 - (sum(pmin(value_MAXE[, 2+scenario], 1.0) * value_MAXE[, 2]) / sum(value_MAXE[, 2]))^z
extinction_thomas[scenario-4, 3] <- 1.0 - (sum(pmin(value_MAXE[, 2+scenario+16*4], 1.0) * value_MAXE[, 2]) / sum(value_MAXE[, 2]))^z
